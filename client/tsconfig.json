    //   "@/layouts/*": ["/layouts/*"],
    //   "@/services/*": ["/services/*"],
    //   "@/store/*": ["/store/*"],
    //   "@/sass/*": ["/sass/*"],
    //   "@/assets/*": ["/assets/*"]
    // }
    // "paths": {
    //   "@/assets/*": ["assets/*"],
    //   "@/components/*": ["src/components/*"],
    //   "@/layouts/helpers": ["src/layouts/*"]
    // }
  },
  "include": [".eslintrc", "src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
